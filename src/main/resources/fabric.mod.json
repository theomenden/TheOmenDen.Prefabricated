{
  "schemaVersion": 1,
  "id": "prefab",
  "version": "${version}",

  "name": "Prefab",
  "description": "Prefab is a mod which contains multiple pre-designed building for you to place in the world",
  "authors": [
    "WuestMan",
    "Kaojinn",
    "NerdySpider",
    "Dqu1J"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/prefab-fabric",
    "sources": "https://github.com/Brian-Wuest/MC-Prefab-Fabric"
  },

  "license": "MIT",
  "icon": "assets/prefab/prefabthumb.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.wuest.prefab.Prefab"
    ],
    "client": [
      "com.wuest.prefab.PrefabClient"
    ],
    "modmenu": [
      "com.wuest.prefab.config.PrefabModMenuIntegration"
    ]
  },
  "accessWidener": "prefab.accesswidener",
  "mixins": [
    "prefab.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "flamingo": "*"
  }
}
